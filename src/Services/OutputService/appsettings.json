{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MongoDB": "localhost",
    "Kafka": "localhost:9092"
  },
  "MassTransit": {
    "UseKafka": true,
    "Kafka": {
      "Server": "localhost:9092"
    }
  },
  "Hazelcast": {
    "Server": "localhost:5701",
    "ClusterName": "data-processing-cluster",
    "CacheTTLHours": 1
  },
  "OpenTelemetry": {
    "OtlpEndpoint": "http://localhost:4317",
    "EnableMetrics": true,
    "EnableTraces": true,
    "EnableLogs": true,
    "SamplingRatio": 1.0,
    "EnableConsoleExporter": false
  },
  "Output": {
    "RetryAttempts": 3,
    "RetryDelayMs": 1000,
    "EnableMetrics": true
  }
}
