<Project>
  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
  </PropertyGroup>

  <ItemGroup>
    <!-- Core Data Processing -->
    <PackageVersion Include="CsvHelper" Version="33.1.0" />
    <PackageVersion Include="EPPlus" Version="8.3.1" />
    <PackageVersion Include="ExcelDataReader" Version="3.8.0" />
    <PackageVersion Include="ExcelDataReader.DataSet" Version="3.8.0" />
    <PackageVersion Include="Newtonsoft.Json" Version="13.0.4" />
    <PackageVersion Include="Newtonsoft.Json.Schema" Version="4.0.1" />
    <PackageVersion Include="NJsonSchema" Version="11.5.2" />

    <!-- Database & Caching -->
    <PackageVersion Include="MongoDB.Entities" Version="24.1.1" />
    <PackageVersion Include="Hazelcast.Net" Version="5.5.1" />

    <!-- Messaging -->
    <PackageVersion Include="MassTransit" Version="8.5.7" />
