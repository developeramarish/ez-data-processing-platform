      "SchemaVersion": "v1.0",
      "Status": 1,
      "JsonSchemaContent": "{\n  \"$schema\": \"https://json-schema.org/draft/2020-12/schema\",\n  \"type\": \"object\",\n  \"properties\": {\n    \"reportId\": {\n      \"type\": \"string\",\n      \"description\": \"מזהה דוח\",\n      \"minLength\": 10,\n      \"maxLength\": 10\n    },\n    \"reportDate\": {\n      \"type\": \"string\",\n      \"description\": \"תאריך דוח\"\n    },\n    \"reportPeriod\": {\n      \"type\": \"object\",\n      \"description\": \"תקופת דיווח\",\n      \"properties\": {\n        \"startDate\": {\n          \"type\": \"string\",\n          \"description\": \"תאריך התחלה\"\n        },\n        \"endDate\": {\n          \"type\": \"string\",\n          \"description\": \"תאריך סיום\"\n        },\n        \"quarter\": {\n          \"type\": \"string\",\n          \"description\": \"רבעון\"\n        }\n      }\n    },\n    \"revenue\": {\n      \"type\": \"object\",\n      \"description\": \"הכנסות\",\n      \"properties\": {\n        \"total\": {\n          \"type\": \"number\",\n          \"description\": \"סה״כ הכנסות\"\n        },\n        \"byCategory\": {\n          \"type\": \"object\",\n          \"description\": \"לפי קטגוריה\"\n        }\n      }\n    },\n    \"expenses\": {\n      \"type\": \"object\",\n      \"description\": \"הוצאות\",\n      \"properties\": {\n        \"total\": {\n          \"type\": \"number\",\n          \"description\": \"סה״כ הוצאות\"\n        },\n        \"byCategory\": {\n          \"type\": \"object\",\n          \"description\": \"לפי קטגוריה\"\n        }\n      }\n    },\n    \"profitLoss\": {\n      \"type\": \"object\",\n      \"description\": \"רווח והפסד\",\n      \"properties\": {\n        \"grossProfit\": {\n          \"type\": \"number\",\n          \"description\": \"רווח גולמי\"\n        },\n        \"netProfit\": {\n          \"type\": \"number\",\n          \"description\": \"רווח נקי\"\n        },\n        \"margin\": {\n          \"type\": \"number\",\n          \"description\": \"אחוז רווח\"\n        }\n      }\n    },\n    \"taxes\": {\n      \"type\": \"object\",\n      \"description\": \"מיסים\",\n      \"properties\": {\n        \"amount\": {\n          \"type\": \"number\",\n          \"description\": \"סכום מס\"\n        },\n        \"rate\": {\n          \"type\": \"number\",\n          \"description\": \"שיעור מס\"\n        }\n      }\n    },\n    \"companyId\": {\n      \"type\": \"string\",\n      \"description\": \"מזהה חברה\"\n    },\n    \"preparedBy\": {\n      \"type\": \"string\",\n      \"description\": \"הוכן על ידי\"\n    },\n    \"approvedBy\": {\n      \"type\": \"string\",\n      \"description\": \"אושר על ידי\"\n    },\n    \"status\": {\n      \"type\": \"string\",\n      \"description\": \"סטטוס דוח\",\n      \"enum\": [\n        \"פעיל\",\n        \"לא פעיל\",\n        \"היסטורי\",\n        \"עתידי\"\n      ]\n    }\n  },\n  \"required\": [\n    \"reportId\",\n    \"reportDate\",\n    \"companyId\",\n    \"status\"\n  ]\n}",
